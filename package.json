    "watch:build": "tsc --watch",
    "watch:server": "nodemon './dist/index.js' --watch './dist'",
    "prepublish": "npm-run-all clean build build:defs",
    "test": "NODE_TEST=true mocha -r ts-node/register src/**/*.spec.ts"
  },
  "files": [
    "dist/**/*"
  ],
